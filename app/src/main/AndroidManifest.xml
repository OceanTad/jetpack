<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sd.jetpack">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".LApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".custom_life.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".custom_life.MusicActivity"/>
        <activity android:name=".life.LifeStateActivity" />
        <activity android:name=".jet_lifecycle.JetActivity" />
        <activity android:name=".vm.OnSellActivity" />
        <activity android:name=".livedata_viewmodel_databing.LvdActivity" />
        <activity android:name=".databinding.DbindingActivity" />

    </application>

</manifest>